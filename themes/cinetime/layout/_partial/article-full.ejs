<div class="container-fluid">
	<div class="row">
		<div class="col-md-8 col-xs-12" id="video_et_description">
					<div class="videoDescription">
				        <h1 class="py-2"><%- item.title %></h1>
				    </div>


					<nav class="nav nav-tabs" id="myTab" role="tablist">
						<% if (item.youtube_url) { %>
					  <a class="nav-item nav-link active" id="nav-youtube-tab" data-toggle="tab" href="#nav-youtube" role="tab" aria-controls="nav-youtube" aria-selected="true">Youtube <i class="fa fa-youtube-play mx-1" aria-hidden="true"></i></a>
						<% } if (item.webtorrent_magnet) { %>
						<a class="nav-item nav-link <% if (!item.youtube_url) { %> active <% } %>" id="nav-webtorrent-tab" data-toggle="tab" href="#nav-webtorrent" role="tab" aria-controls="nav-webtorrent" aria-selected="false">Webtorrent <i class="fa fa-heart mx-1" aria-hidden="true"></i></a></a>
						<% } if (item.vimeo_url) { %>
						<a class="nav-item nav-link" id="nav-vimeo-tab" data-toggle="tab" href="#nav-vimeo" role="tab" aria-controls="nav-vimeo" aria-selected="false">Vimeo <i class="fa fa-vimeo-square mx-1" aria-hidden="true"></i></a>
						<% } %>
					</nav>

					<script src="<%- url_for('bower_components/video.js/dist/video.min.js') %>"></script>
					<script src="<%- url_for('bower_components/videojs-youtube/dist/Youtube.min.js') %>"></script>

					<div class="tab-content" id="nav-tabContent">
					<% if (item.youtube_url) { %>
						<div class="tab-pane fade show active" id="nav-youtube" role="tabpanel" aria-labelledby="nav-youtube-tab">
							<div class="video">
								<video id="my-video" class="video-js vjs-16-9" controls preload="auto" width="640px" height="267px" poster="<%- url_for('img/')%>poster_<%- item.img_name %>" data-setup='{ "techOrder": ["youtube"], "sources": [{ "type": "video/youtube", "src": "<%- item.youtube_url %>"}] }'>
								</video>
							</div>
						</div>
						<% } if (item.webtorrent_magnet) { %>
						<div class="tab-pane fade <% if (!item.youtube_url) { %> show active <% } %>" id="nav-webtorrent" role="tabpanel" aria-labelledby="nav-webtorrent-tab">
							<div class="video" id="video_webtorrent" >

							</div>
						</div>
						<script>
								setTimeout(()=>{
									// console.log('setting the poster image...');
									document.getElementById('video_webtorrent').lastChild.setAttribute("poster","<%- url_for('img/')%>poster_<%- item.img_name %>")
								}, 5000)

						</script>
						<% } if (item.vimeo_url) { %>
						<div class="tab-pane fade" id="nav-vimeo" role="tabpanel" aria-labelledby="nav-vimeo-tab">

						</div>
						<% } %>
					</div>

					<span id="magnet" class="d-none"><%- item.webtorrent_magnet %></span>

				    <div class="videoDescription">
				        <%- item.content %>
				    </div>




				    <div id="rs">
							<!-- Buttons start here. Copy this ul to your document. -->
							<ul class="rrssb-buttons clearfix">
								<li class="rrssb-facebook">
									<!--  Replace with your URL. For best results, make sure you page has the proper FB Open Graph tags in header:
												https://developers.facebook.com/docs/opengraph/howtos/maximizing-distribution-media-content/ -->
									<a href="https://www.facebook.com/sharer/sharer.php?u=<%- url %>" class="popup">
										<span class="rrssb-icon">
											<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 29 29"><path d="M26.4 0H2.6C1.714 0 0 1.715 0 2.6v23.8c0 .884 1.715 2.6 2.6 2.6h12.393V17.988h-3.996v-3.98h3.997v-3.062c0-3.746 2.835-5.97 6.177-5.97 1.6 0 2.444.173 2.845.226v3.792H21.18c-1.817 0-2.156.9-2.156 2.168v2.847h5.045l-.66 3.978h-4.386V29H26.4c.884 0 2.6-1.716 2.6-2.6V2.6c0-.885-1.716-2.6-2.6-2.6z"/></svg>
										</span>
										<span class="rrssb-text">facebook</span>
									</a>
								</li>
								<li class="rrssb-twitter">
									<!-- Replace href with your Meta and URL information  -->
									<a href="https://twitter.com/intent/tweet?text=<%- url %>"
									class="popup">
										<span class="rrssb-icon">
											<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 28 28"><path d="M24.253 8.756C24.69 17.08 18.297 24.182 9.97 24.62a15.093 15.093 0 0 1-8.86-2.32c2.702.18 5.375-.648 7.507-2.32a5.417 5.417 0 0 1-4.49-3.64c.802.13 1.62.077 2.4-.154a5.416 5.416 0 0 1-4.412-5.11 5.43 5.43 0 0 0 2.168.387A5.416 5.416 0 0 1 2.89 4.498a15.09 15.09 0 0 0 10.913 5.573 5.185 5.185 0 0 1 3.434-6.48 5.18 5.18 0 0 1 5.546 1.682 9.076 9.076 0 0 0 3.33-1.317 5.038 5.038 0 0 1-2.4 2.942 9.068 9.068 0 0 0 3.02-.85 5.05 5.05 0 0 1-2.48 2.71z"/></svg>
										</span>
										<span class="rrssb-text">twitter</span>
									</a>
								</li>
								<li class="rrssb-pinterest">
									<!-- Replace href with your meta and URL information.  -->
									<a href="http://pinterest.com/pin/create/button/?url=<%- encodeURIComponent(url) %>&amp;media=<%- encodeURIComponent(config.url + url_for('img/') + 'poster_' + item.img_name) %>&amp;description=<%-encodeURIComponent(item.title + ', ' + item.author + ' (' + item.year+ ')' )%>.">
										<span class="rrssb-icon">
											<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 28 28"><path d="M14.02 1.57c-7.06 0-12.784 5.723-12.784 12.785S6.96 27.14 14.02 27.14c7.062 0 12.786-5.725 12.786-12.785 0-7.06-5.724-12.785-12.785-12.785zm1.24 17.085c-1.16-.09-1.648-.666-2.558-1.22-.5 2.627-1.113 5.146-2.925 6.46-.56-3.972.822-6.952 1.462-10.117-1.094-1.84.13-5.545 2.437-4.632 2.837 1.123-2.458 6.842 1.1 7.557 3.71.744 5.226-6.44 2.924-8.775-3.324-3.374-9.677-.077-8.896 4.754.19 1.178 1.408 1.538.49 3.168-2.13-.472-2.764-2.15-2.683-4.388.132-3.662 3.292-6.227 6.46-6.582 4.008-.448 7.772 1.474 8.29 5.24.58 4.254-1.815 8.864-6.1 8.532v.003z"/></svg>
										</span>
										<span class="rrssb-text">pinterest</span>
									</a>
								</li>
								<li class="rrssb-googleplus">
									<!-- Replace href with your meta and URL information.  -->
									<a href="https://plus.google.com/share?url=<%- url %>" class="popup">
										<span class="rrssb-icon">
											<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24"><path d="M21 8.29h-1.95v2.6h-2.6v1.82h2.6v2.6H21v-2.6h2.6v-1.885H21V8.29zM7.614 10.306v2.925h3.9c-.26 1.69-1.755 2.925-3.9 2.925-2.34 0-4.29-2.016-4.29-4.354s1.885-4.353 4.29-4.353c1.104 0 2.014.326 2.794 1.105l2.08-2.08c-1.3-1.17-2.924-1.883-4.874-1.883C3.65 4.586.4 7.835.4 11.8s3.25 7.212 7.214 7.212c4.224 0 6.953-2.988 6.953-7.082 0-.52-.065-1.104-.13-1.624H7.614z"/></svg>            </span>
										<span class="rrssb-text">google+</span>
									</a>
								</li>
								<li class="rrssb-email">
									<!-- Replace subject with your message using URL Endocding: http://meyerweb.com/eric/tools/dencoder/ -->
									<a href="mailto:?Subject=<%-encodeURIComponent(item.title + ', ' + item.author + ' (' + item.year+ ')' )%>&amp;body=<%- encodeURIComponent(url) %>">
										<span class="rrssb-icon">
											<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24"><path d="M21.386 2.614H2.614A2.345 2.345 0 0 0 .279 4.961l-.01 14.078a2.353 2.353 0 0 0 2.346 2.347h18.771a2.354 2.354 0 0 0 2.347-2.347V4.961a2.356 2.356 0 0 0-2.347-2.347zm0 4.694L12 13.174 2.614 7.308V4.961L12 10.827l9.386-5.866v2.347z"/></svg>
										</span>
										<span class="rrssb-text">email</span>
									</a>
								</li>
								<li class="rrssb-tumblr">
									<a href="http://tumblr.com/share/link?url=<%- url %>&name=<%- item.title + ', ' + item.author + ' (' + item.year+ ')' %>">
										<span class="rrssb-icon">
											<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 28 28"><path d="M18.02 21.842c-2.03.052-2.422-1.396-2.44-2.446v-7.294h4.73V7.874H15.6V1.592h-3.714s-.167.053-.182.186c-.218 1.935-1.144 5.33-4.988 6.688v3.637h2.927v7.677c0 2.8 1.7 6.7 7.3 6.6 1.863-.03 3.934-.795 4.392-1.453l-1.22-3.54c-.52.213-1.415.413-2.115.455z"/></svg>
										</span>
										<span class="rrssb-text">tumblr</span>
									</a>
								</li>
							</ul>
							<!-- Buttons end here -->

				    </div>

					 <!-- Tags and Categories links -->
				<!--      partial('article-tags', {item: item}) -->
				<!--     <%- partial('article-categories', {item: item}) %>-->

				<script src="<%- url_for('js/social.js') %>"></script>
		</div>

		<div class="col-md-4 col-xs-12">
				<div class="d-flex flex-column">
					<!-- <div class="media">
					<img class="mr-3" src="/img/lagreve.jpg" alt="Generic placeholder image">
							<div class="media-body">
									<h6 class="mt-0">Media heading</h6>
							</div>
					</div> -->
					<div class="row">
						<h5 class="m-4">Ã€ suivre</h5>
					</div>
								<ul class="list-unstyled">
								<% list_related_posts({maxCount: 10}).forEach(function(post) { %>
									<li class="media mb-3">
										<a href="<%- url_for(post.path) %>" class="next-link" title="<%- post.title %>">
											<img class="mr-3" width="168" src="<%- url_for('img/')%>next_<%- post.img_name %>" alt="<%- post.title + ', ' + site.data.metadata.alt_intro %>">
										</a>
										<div class="media-body">
											<a href="<%- url_for(post.path) %>" class="underline-not" title="<%- post.title %>">
											<h5 class="mt-0 mb-1"><%- post.title %></h5>
											<p><%- _.startCase(post.author) %></p>
											</a>
										</div>
									</li>
								<% });	%>
								</ul>
				</div>
		</div>
	</div>
</div>
